<section class="profile-authentication-area ptb-100">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-md-12">
                <div class="login-form">
                    <h2>Register</h2>

                    {{ form_start(forme,{attr:{novalidate:'novalidate'}})}}
                        <div class="form-group">
                            <label>Firstname</label>
                            {{ form_widget(forme.firstname,{'att':{'class':'form-control'}}) }}
                        </div>

                        <div class="form-group">
                            <label>Lastname</label>
                            {{ form_widget(forme.lastname,{'att':{'class':'form-control'}}) }}
                        </div>
                    <div class="form-group">
                        <label>Email</label>
                        {{ form_widget(forme.email,{'att':{'class':'form-control'}}) }}
                    </div>
                    <div class="form-group">
                        <label>Password</label>
                        {{ form_widget(forme.password.first,{'att':{'class':'form-control'}}) }}
                    </div>
                    <div class="form-group">
                        <label>Reapet Password</label>
                        {{ form_widget(forme.password.second,{'att':{'class':'form-control'}}) }}
                    </div>
                    <div class="form-group">
                        <label>Phone Number</label>
                        {{ form_widget(forme.phoneNumber,{'att':{'class':'form-control'}}) }}
                    </div>
                    <div class="form-group">
                        <label>Role</label>
                        {{ form_widget(forme.idRole,{'att':{'class':'form-control'}}) }}
                    </div>
                    <div class="form-group">
                        <label>Captcha</label>
                        <label for="exempleFormControlInput1">Are you a Robot ?</label>

                        {{ form_widget(forme.captchaCode,{'att':{'class':'form-control'}}) }}
                    </div>



                        <button type="submit">Log In</button>
                    {{ form_end(forme) }}
